{
  "name": "tf2-rcon-extension",
  "displayName": "TF2 RCON Console",
  "description": "Manage TF2 server via RCON from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openTF2Console",
        "title": "Open TF2 Console"
      },
      {
        "command": "extension.setRconPassword",
        "title": "Set RCON Password"
      },
      {
        "command": "extension.setRconIp",
        "title": "Set RCON IP Address"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "devDependencies": {
    "@types/node": "^14.0.27",
    "typescript": "^4.0.3",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "rcon-client": "^4.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/titaniummachine1/Lmaobox_Console_Debugger"
  }
}
